  object zqryUsuarios: TZQuery
    Connection = dtmPrincipal.tzConexaoPrincipal
    SQL.Strings = (
      
        'SELECT usuario_id,usuario_nome,usuario_email,usuario_telefone,us' +
        'uario_senha'
      ' FROM usuario ORDER BY usuario_id;'
      '')
